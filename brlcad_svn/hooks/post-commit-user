#!/bin/sh

curl -s http://brlcad.org/svn/statusBuild.php
/usr/lib64/subversion/contrib/hook-scripts/ciabot_svn.py --revisionURI="http://sourceforge.net/p/brlcad/code/%(revision)s" --repositoryURI=http://svn.code.sf.net/p/brlcad/code/ "$1" "$2" "BRL-CAD"
/usr/lib64/subversion/contrib/hook-scripts/svnnotify --set-sender --repos-path "$1" --revision "$2" --to "brlcad-commits@lists.sourceforge.net" --subject-prefix "SF.net SVN: brlcad:" --subject-cx --no-first-line --with-diff --viewcvs-url "http://sourceforge.net/p/brlcad/code/%s" --user-domain "users.sourceforge.net" --footer "This was sent by the SourceForge.net collaborative development platform, the world's largest Open Source development site." -l /usr/bin/svnlook --max-diff-length 100000
exit 0
