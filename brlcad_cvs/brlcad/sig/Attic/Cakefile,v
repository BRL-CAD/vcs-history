@/*
 *			sig/Cakefile
 */

#define SRCDIR	sig

#define PRODUCTS	a-d d-a i-d d-i bw-d d-bw f-d d-f f-i i-f c-d i-a \
			u-a u-d u-f u-bw d-u \
			dwin dauto dmod dfft dconv imod umod \
			ddisp fhor \
			dstat dsel \
			dpeak syn damdf dauto2 ihist istat ustat

#define	SRCSUFF	.c
#define MANSECTION	1

#include "../Cakefile.defs"
#include "../Cakefile.prog"


a-d: a-d.o LIBBU_DEP
	CC LDFLAGS -o a-d a-d.o LIBBU LIBBU_LIBES LIBES

d-a: d-a.o
	CC LDFLAGS -o d-a d-a.o LIBES

i-d: i-d.o
	CC LDFLAGS -o i-d i-d.o LIBES

d-i: d-i.o
	CC LDFLAGS -o d-i d-i.o LIBES

