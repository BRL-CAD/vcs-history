	CC LDFLAGS -o bw-d bw-d.o LIBES

d-bw: d-bw.o
	CC LDFLAGS -o d-bw d-bw.o LIBES

d-f: d-f.o
	CC LDFLAGS -o d-f d-f.o LIBES

f-d: f-d.o
	CC LDFLAGS -o f-d f-d.o LIBES

f-i: f-i.o
	CC LDFLAGS -o f-i f-i.o LIBES

i-f: i-f.o
	CC LDFLAGS -o i-f i-f.o LIBES

c-d: c-d.o
	CC LDFLAGS -o c-d c-d.o LIBES

i-a: i-a.o
	CC LDFLAGS -o i-a i-a.o LIBES

u-a: u-a.o
	CC LDFLAGS -o u-a u-a.o LIBES

u-d: u-d.o
	CC LDFLAGS -o u-d u-d.o LIBES

u-f: u-f.o
