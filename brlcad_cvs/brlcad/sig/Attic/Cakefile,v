	CC LDFLAGS -o u-bw u-bw.o LIBES

/* Processors */

dwin: dwin.o coswin.o hamwin.o
	CC LDFLAGS -o dwin dwin.o coswin.o hamwin.o LIBES

dauto: dauto.o
	CC LDFLAGS -o dauto dauto.o LIBES

dmod: dmod.o
