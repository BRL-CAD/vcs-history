#ifndef lint
static char RCSid[] = "@@(#)$Header: /c/CVS/brlcad/proc-db/platetest.c,v 11.2 2000/01/31 15:27:43 jra Exp $ (BRL)";
#endif

#include "conf.h"

#include <stdio.h>
#include <math.h>
#include "machine.h"
#include "externs.h"
#include "vmath.h"
#include "rtlist.h"
#include "wdb.h"

main( argc, argv )
int argc;
char *argv[];
{
	point_t min, max;

	mk_id( stdout, "Plate mode test" );

	/* first an rpp */
	VSET( min, 10.0, 20.0, 30.0 );
	VSET( max, 40.0, 50.0, 60.0 );
	mk_rpp( stdout, "an_rpp", min, max  );
	mk_rpp( stdout, "another_rpp", min, max  );

	mk_fgp( stdout, "fgp.1", "an_rpp", 5.0, 1 );
	mk_fgp( stdout, "fgp.2", "another_rpp", 5.0, 2 );
}
@


