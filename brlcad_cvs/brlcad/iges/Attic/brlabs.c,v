# include "config.h"
#endif



#include <stdio.h>

#include "machine.h"

fastf_t brlabs( a )
register const fastf_t a;
{
	register fastf_t b;

	if( a > 0 )
		b = a;
	else
		b = (-a);

	return( b );
}
@


