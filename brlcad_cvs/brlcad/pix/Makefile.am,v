14.8
log
@remove the MAINTAINERCLEAN rules, they're never really used regardless of the debate over whether the Makefile.in files belong there or not
@
text
@# $Id: Makefile.am,v 14.7 2007/01/27 01:41:30 brlcad Exp $

BENCHMARK_PIX = \
	bldg391.pix \
	m35.pix \
	moss.pix \
	sphflake.pix \
	star.pix \
	world.pix


PIX = \
	${BENCHMARK_PIX} \
	golf_ball.pix \
	lgt1.pix \
	lgt2.pix \
	lgt3.pix \
	mworld.pix \
	toilet.pix

LOG = \
	bldg391.log \
	m35.log \
	moss.log \
	sphflake.log \
	star.log \
	world.log

SAVEVIEWS = \
	cube.rt

EXTRA_DIST = \
	${BENCHMARK_PIX} \
	${PIX} \
	${LOG} \
	${SAVEVIEWS}

pixdir = $(BRLCAD_DATA)/pix
pix_DATA = \
	${BENCHMARK_PIX} \
	${LOG}

include $(top_srcdir)/misc/Makefile.defs
@


