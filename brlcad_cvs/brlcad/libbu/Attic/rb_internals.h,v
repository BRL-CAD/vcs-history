#define	RB_CKMAG(p, m, _s)					\
    if ((p) == 0)						\
    {								\
	rt_log("Error: Null %s pointer, file %s, line %d\n",	\
	    (_s), __FILE__, __LINE__);				\
	exit (0);						\
    }								\
    else if (*((long *)(p)) != (m))				\
    {								\
	rt_log(							\
	    "Error: Bad %s pointer x%x s/b x%x was x%x, file %s, line %d\n", \
	    (_s), (p), (m), *((long *)(p)), __FILE__, __LINE__);\
	exit (0);						\
    }
