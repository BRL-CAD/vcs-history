#ifndef lint
static const char libbu_memset_RCSid[] = "@@(#)$Header: /n/xoff/cvs/brlcad/libbu/memset.c,v 1.3 2000/07/07 05:20:12 cjohnson Exp $ (ARL)";
#endif

#if defined(BSD) && (BSD < 43)

char *
memset( s, c, n )
char	*s;
register char	c;
register int	n;
{
	register char *p=s;

	if (p)
		while (n-- > 0)
			*p++ = c;

	return s;
}

#endif
@


