/*
 *			T A B _ S I N
 *
 * Table lookup sine function.
 */
double
tab_sin(angle)
double angle;
{
#define	TABSIZE	512
d34 515
a548 3
	static	int	init = 0;
	static	double	table[TABSIZE];
	register int	i;
d550 515
a1064 55
	if (init == 0) {
		for( i = 0; i < TABSIZE; i++ )
			table[i] = sin( TWOPI * i / TABSIZE );
		init++;
	}

	if (angle > 0)
		return( table[(int)((angle / TWOPI) * TABSIZE + 0.5) % TABSIZE] );
	else
		return( -table[(int)((-angle / TWOPI) * TABSIZE + 0.5) % TABSIZE] );
}

/*
 *  			R A N D 0 T O 1
 *
 *  Returns a random number in the range 0 to 1
 *
 *  This really wants to become a table-driven operation.
 */
double rand0to1()
{
#ifdef BENCHMARK
	return(0.5);
#else
	FAST fastf_t f;

#ifdef BSD
	f = ((double)random()) *
	/* / (double)0x7FFFFFFFL */
		0.00000000046566128752457969241057508271679984532147;
#else
	f = ((double)rand()) *
	/* / (double)0x7FFFL */
		0.00003051850947599719229712820825;
#endif
	if( f > 1.0 || f < 0 )  {
		rt_log("rand0to1 out of range\n");
		return(0.42);
	}
	return(f);
#endif
}

/*
 *  			R A N D _ H A L F
 *
 *  Returns a random number in the range -0.5 to +0.5
 */
double rand_half()  {
#ifdef BENCHMARK
	return(0.0);
#else
	return(rand0to1()-0.5);
#endif
}
