/*
 *			F A C E
 *
 * This routine traces one face of an ARB8
 */
static void
face( valp, a, b, c, d )
register fastf_t *valp;
{
	DM_GOTO( &valp[a*3], PEN_UP );
	DM_GOTO( &valp[b*3], PEN_DOWN );
	DM_GOTO( &valp[c*3], PEN_DOWN );
	DM_GOTO( &valp[d*3], PEN_DOWN );
@


