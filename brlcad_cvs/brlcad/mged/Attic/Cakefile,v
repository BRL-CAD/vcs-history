11.51
log
@merge of AUTOCONF branch in to HEAD
@
text
@/*
 *			mged/Cakefile
 *
 *  $Header: /cvs/brlcad/mged/Cakefile,v 11.50 2001/06/01 19:23:22 bparker Exp $
 */
#define       CONFIGDEFS      MGED_CONFIG "-Dyylex=cmd_nop"

#define SRCDIR	mged
#define PRODUCTS	mged cad_boundp cad_parea
#define STATIC_PRODUCTS mged.tcl
#define	SRCSUFF	.c
#define MANSECTION	1

#define FILES	ged dodraw dozoom titles scroll \
			usepen dir columns mover tedit \
			cmd chgview chgmodel chgtree \
			rtif anal typein predictor \
			buttons menu adc edsol edarb edpipe edars \
			polyif red \
			mater plot overlay inside \
			concat utility1 utility2 track arbs \
			facedef set animedit \
			solids_on_ray history \
			TCP_FILES attach dm-plot dm-ps muves \
			MGED_OBJS vdraw fbserv rect grid axes qray \
			color_scheme vparse share bodyio

/*#define USING_NEW_SO*/
#include "../Cakefile.defs"
#include "../Cakefile.prog"

#if HAS_TCP
#    define COMPLETE_LIBES	\
	LIBFB LIBPKG LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTERMIO MGED_LIBES LIBES
#    define	TCP_FILES	vrlink
#else
#    define COMPLETE_LIBES	\
	LIBFB LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTERMIO MGED_LIBES LIBES
#    define	TCP_FILES	/**/
#endif

mged:	OBJ LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBWDB_DEP LIBTCL_DEP LIBTK_DEP LIBDM_DEP LIBFB_DEP
	sh ../sh/newvers.sh version "Geometry Editor (MGED)"
	CC CFLAGS -c vers.c
	CC -o mged LDFLAGS OBJ COMPLETE_LIBES vers.o

cad_boundp: cad_boundp.o LIBBU_DEP
	CC -o cad_boundp LDFLAGS cad_boundp.o LIBBU LIBBU_LIBES LIBES

cad_parea: cad_parea.o LIBBU_DEP
	CC -o cad_parea LDFLAGS cad_parea.o LIBBU LIBBU_LIBES LIBES

#include "../Cakefile.rules"
@


