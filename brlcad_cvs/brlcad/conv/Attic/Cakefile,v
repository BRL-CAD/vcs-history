@/*
 *			conv/Cakefile
 *
 *  $Header: /cvs/brlcad/conv/Cakefile,v 11.41 2003/10/02 15:05:18 jra Exp $
 */

#define SRCDIR	conv
#define PRODUCTS	asc2g g2asc conv-vg2g pix2asc asc2pix euclid-g \
			g-euclid euclid_format euclid_unformat tankill-g g-tankill \
			viewpoint-g dxf-g g-nmg g-euclid1 fast4-g proe-g g-nff \
			asc-nmg nmg-rib g-vrml g-x3d g-acad cy-g nastran-g g-wave g-stl \
			nmg-bot g-shell.rect stl-g poly-bot dbupgrade enf-g bot_shell-vtk \
			g-dxf ply-g
#define	SRCSUFF	.c
#define MANSECTION	1

#define STATIC_PRODUCTS ../SRCDIR/g-xxx.c ../SRCDIR/g-xxx_facets.c

#include "../Cakefile.defs"
#include "../Cakefile.prog"

/* Explicit composition of each product */

g-stl: g-stl.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS g-stl.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o g-stl

g-dxf: g-dxf.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS g-dxf.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o g-dxf

g-xxx: g-xxx.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS g-xxx.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o g-xxx

g-xxx_facets: g-xxx_facets.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS g-xxx_facets.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o g-xxx_facets

bot_shell-vtk: bot_shell-vtk.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBSYSV_DEP
	CC LDFLAGS bot_shell-vtk.o LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES LIBSYSV -o bot_shell-vtk

enf-g: enf-g.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP LIBSYSV_DEP
	CC LDFLAGS enf-g.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES LIBSYSV -o enf-g

ply-g: ply-g.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP LIBSYSV_DEP
	CC LDFLAGS ply-g.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES LIBSYSV -o ply-g

poly-bot: poly-bot.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBWDB_DEP LIBTCL_DEP
	CC LDFLAGS poly-bot.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o poly-bot

nmg-bot: nmg-bot.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS nmg-bot.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o nmg-bot

g-sgp: g-sgp.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS g-sgp.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o g-sgp

nmg-sgp: nmg-sgp.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS nmg-sgp.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -lgen -o nmg-sgp

nastran-g: nastran-g.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS nastran-g.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o nastran-g

g-acad: g-acad.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS g-acad.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o g-acad

g-nff: g-nff.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS g-nff.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o g-nff

g-wave: g-wave.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS g-wave.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o g-wave

g-shell.rect: g-shell.rect.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS g-shell.rect.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o g-shell.rect

proe-g: proe-g.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP LIBSYSV_DEP
	CC LDFLAGS proe-g.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES LIBSYSV -o proe-g

stl-g: stl-g.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP LIBSYSV_DEP
	CC LDFLAGS stl-g.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES LIBSYSV -o stl-g

