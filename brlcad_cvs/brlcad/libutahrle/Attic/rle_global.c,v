a90 3
#if defined(__linux__) || ( defined(__FreeBSD__) && __FreeBSD__ >=5 )
    NULL,			/* In LINUX, now "extern FILE *stdout" */
#elif defined(stdout)
a91 3
#else
    NULL,
#endif
