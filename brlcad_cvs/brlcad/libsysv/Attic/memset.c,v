#ifndef lint
static char RCSid[] = "@@(#)$Header: /n/xoff/cvs/brlcad/libsysv/memset.c,v 11.4 2004/04/05 07:41:29 morrison Exp $ (BRL)";
#endif


char *
memset(char *s, register char c, register int n)
{
	register char *p=s;

	if (p)
		while (n-- > 0)
			*p++ = c;

	return s;
}
@


