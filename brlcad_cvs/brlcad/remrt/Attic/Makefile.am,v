	rtsrv.c \
	$(top_srcdir)/rt/do.c \
	$(top_srcdir)/rt/opt.c \
	$(top_srcdir)/rt/sh_tcl.c \
	$(top_srcdir)/rt/view.c \
	$(top_srcdir)/rt/worker.c

