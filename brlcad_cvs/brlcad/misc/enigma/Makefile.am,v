14.13
log
@remove the MAINTAINERCLEAN rules, they're never really used regardless of the debate over whether the Makefile.in files belong there or not
@
text
@
bin_PROGRAMS = enigma

engima_SOURCE = enigma.c

dist_man_MANS = enigma.1

EXTRA_DIST = \
	AUTHORS \
	COPYING \
	ChangeLog \
	INSTALL \
	NEWS \
	README \
	configure.ac

DISTCLEANFILES = \
	aclocal.m4 \
	config.log \
	config.status \
	configure

noprod: prodclean

prodclean-recursive:

prodclean: prodclean-recursive
	rm -f enigma

fast: all-am

depends: all-am


# Local Variables:
# mode: Makefile
# tab-width: 8
# indent-tabs-mode: t
# End:
# ex: shiftwidth=8 tabstop=8
@


