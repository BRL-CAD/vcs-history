    constructor {dbOrFile} {}
a97 1
	method shareDb {_db}
a121 1
	variable sharedDb 0
d135 4
a138 11
::itcl::body Db::constructor {dbOrFile} {
    if {[catch {$dbOrFile ls}]} {
	set dbfile $dbOrFile
	set db [subst $this]_db
	wdb_open $db $dbfile
    } else {
	set dbfile ""
	set db $dbOrFile
	set sharedDb 1
    }

d143 1
a143 4
    if {!$sharedDb} {
	rename $db ""
    }

a182 8
::itcl::body Db::shareDb {_db} {
    if {!$sharedDb} {
	rename $db ""
    }

    set db $_db
}

@


