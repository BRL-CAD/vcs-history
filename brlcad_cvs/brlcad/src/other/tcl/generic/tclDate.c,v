       9,    23,     0,     9,     7,    14,     4,     5,    14,    14,
      14,     9,    10,    11,    12,    17,    14,    15,    16,    22,
      18,    19,    20,     7,    22,    14,     4,    15,    26,     7,
      18,     9,    10,     3,    12,    25,    45,    21,    22,    48,
