     -23,     2,   -23,   -22,   -23,    -5,   -23,    -4,   -23,    22,
      -2,   -23,    12,   -23,    38,   -23,   -23,   -23,   -23,   -23,
     -23,   -23,   -23,   -23,   -23,   -23,    30,    11,   -23,   -23,
     -23,    17,    10,   -23,   -23,    35,    40,    -6,    47,   -23,
