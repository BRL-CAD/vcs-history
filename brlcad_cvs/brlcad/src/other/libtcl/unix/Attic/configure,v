sed 's/^/| /' conftest.$ac_ext >&5

tcl_cv_struct_stat64=no
fi
rm -f conftest.$ac_objext conftest.$ac_ext
fi

	if test "x${tcl_cv_struct_stat64}" = "xyes" ; then
