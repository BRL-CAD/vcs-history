  tcl_ok=yes
else
  tcl_ok=no
fi

	fi

	if test "$tcl_ok" = "yes"; then
	    # The space is needed
	    THREADS_LIBS=" -lpthread"
	else
	    echo "$as_me:$LINENO: checking for pthread_mutex_init in -lpthreads" >&5
