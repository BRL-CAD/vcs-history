    echo "$as_me:$LINENO: result: yes" >&5
echo "${ECHO_T}yes" >&6
else
    echo "$as_me:$LINENO: result: broken, using substitute" >&5
echo "${ECHO_T}broken, using substitute" >&6
    LIBOBJS="$LIBOBJS strstr.$ac_objext"
