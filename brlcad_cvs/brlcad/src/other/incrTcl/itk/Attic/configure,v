s,@@itcl_STUB_LIB_PATH@@,$itcl_STUB_LIB_PATH,;t t
s,@@ITCL_INCLUDES@@,$ITCL_INCLUDES,;t t
s,@@TCL_TOP_DIR_NATIVE@@,$TCL_TOP_DIR_NATIVE,;t t
s,@@TCL_GENERIC_DIR_NATIVE@@,$TCL_GENERIC_DIR_NATIVE,;t t
