# ------------------------------------------------------------------------
#       Makefile for demonstation shell of BLT library
# ------------------------------------------------------------------------

!INCLUDE ./win/makedefs

# ------------------------------------------------------------------------
#       Source and target installation directories
# ------------------------------------------------------------------------

srcdir		= .

# ------------------------------------------------------------------------
#       Don't edit anything beyond this point
# ------------------------------------------------------------------------

all:  
	cd $(MAKEDIR)\src
	$(MAKE) -f blt.mak all
	cd $(MAKEDIR)

install: install-all

