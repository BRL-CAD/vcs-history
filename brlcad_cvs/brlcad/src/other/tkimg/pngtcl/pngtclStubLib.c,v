#define USE_TCL_STUBS
#endif

#include "pngtcl.h"

PngtclStubs *pngtclStubsPtr;

/*
 *----------------------------------------------------------------------
 *
 * Png_InitStubs --
 *
 *	Checks that the correct version of Blt is loaded and that it
 *	supports stubs. It then initialises the stub table pointers.
