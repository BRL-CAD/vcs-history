14.4
log
@remove the MAINTAINERCLEAN rules, they're never really used regardless of the debate over whether the Makefile.in files belong there or not
@
text
@# $Id: Makefile.am,v 14.3 2005/07/25 05:49:12 brlcad Exp $

SUBDIRS = \
	doc \
	generic

INSTALL_DIR = ${libdir}/iwidgets${IWIDGETS_VERSION}

brlcadscriptdir = ${INSTALL_DIR}
brlcadscript_SCRIPTS = iwidgets.tcl pkgIndex.tcl license.terms

EXTRA_DIST = \
	$(brlcadscript_SCRIPTS) \
	CHANGES \
	aclocal.m4 \
	demos \
	incoming \
	outgoing \
	tcl.m4 \
	tests \
	win

DISTCLEANFILES = \
	iwidgets.tcl \
	pkgIndex.tcl

install-script-hook:
	ln -fs ${INSTALL_DIR} ${libdir}/iwidgets

include $(top_srcdir)/misc/Makefile.defs

@


