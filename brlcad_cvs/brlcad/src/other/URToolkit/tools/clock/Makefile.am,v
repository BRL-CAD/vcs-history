1.5
log
@remove the MAINTAINERCLEAN rules, they're never really used regardless of the debate over whether the Makefile.in files belong there or not
@
text
@# $Id: Makefile.am,v 1.4 2007/02/03 07:20:29 brlcad Exp $

bin_PROGRAMS = rleClock

rleClock_SOURCES = \
	font.c \
	rleClock.c

noinst_HEADERS = font.h

font.c: ${srcdir}/makeFont ${srcdir}/font.src
	@@SH@@ ${srcdir}/makeFont ${srcdir}/font.src > font.c

rleClock_LDADD = ${RLE}
rleClock_CFLAGS = ${RLE_CPPFLAGS}

EXTRA_DIST = \
	README \
	font.src \
	makeFont

include $(top_srcdir)/misc/Makefile.defs

@


