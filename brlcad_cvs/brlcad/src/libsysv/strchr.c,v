#include "common.h"

/* quell empty-compilation unit warnings */
static const int unused = 0;

/*
 * defined for folks that don't have a system strchr()
 */
#ifndef HAVE_STRCHR

char *
strchr(register char *sp, register char c)
{
    do {
	if( *sp == c )
	    return( sp );
    }  while( *sp++ );
    return( (char *)0 );
}

#endif

/*
 * Local Variables:
 * mode: C
 * tab-width: 8
 * c-basic-offset: 4
 * indent-tabs-mode: t
 * End:
 * ex: shiftwidth=4 tabstop=8
 */
@


