	cscript //nologo ..\..\misc\win32-msvc\vers.vbs libsysv_version The BRL-CAD System V Compatibility Library > $(ProjDir)\vers.c

# End Custom Build

!ELSEIF  "$(CFG)" == "libsysv - Win32 Release Multithreaded"

