14.4
log
@remove the libutahrle headers (utah raster toolkit) from our include/ directory, and move them up to src/other/libutahrle/include.  let configure set RLE_CPPFLAGS and set accordingly amongst the various tools/libraries that need to know the path.
@
text
@d1 1
a1 9
# $Id: Makefile.am,v 14.3 2007/01/27 01:41:38 brlcad Exp $

if ONLY_BENCHMARK

lib_LTLIBRARIES =
man_MANS =
BUILT_SOURCES =

else !ONLY_BENCHMARK
a4 1
BUILT_SOURCES = vers.c
d6 1
a6 1
liborle_la_SOURCES = $(SRCS) vers.c
a8 8
endif
# end ONLY_BENCHMARK

SRCS = rle.c

vers.c: $(SRCS)
	@@SH@@ $(top_srcdir)/sh/vers.sh  liborle_version "University of Utah (Old) RLE Library, Version 2" > $@@

a10 2
CLEANFILES = $(BUILT_SOURCES)
DISTCLEANFILES = version
@


