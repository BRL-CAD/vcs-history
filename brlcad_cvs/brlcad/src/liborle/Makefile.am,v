14.6
log
@remove the MAINTAINERCLEAN rules, they're never really used regardless of the debate over whether the Makefile.in files belong there or not
@
text
@# $Id: Makefile.am,v 14.5 2007/04/16 23:51:45 brlcad Exp $

lib_LTLIBRARIES = liborle.la
man_MANS = liborle.3

liborle_la_SOURCES = rle.c
liborle_la_LDFLAGS = -version-info 19:1

EXTRA_DIST = $(man_MANS)

include $(top_srcdir)/misc/Makefile.defs
@


