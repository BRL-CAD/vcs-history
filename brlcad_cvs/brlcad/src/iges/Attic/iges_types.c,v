#ifndef lint
static const char RCSid[] = "@@(#)$Header: /cvsroot/brlcad/brlcad/src/iges/iges_types.c,v 14.5 2005/10/23 04:44:31 brlcad Exp $ (BRL)";
#endif

#include "./iges_struct.h"
#include "./iges_extern.h"

char *
iges_type( type )
int type;
{
	int i;
	int type_no=0;

	for( i=1 ; i<ntypes ; i++ )
	{
		if( typecount[i].type == type )
		{
			type_no = i;
			break;
		}
	}

	return( typecount[type_no].name );
}

