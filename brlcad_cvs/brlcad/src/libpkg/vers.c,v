/** @@file vers.c
 *
 * version information about LIBPKG
 *
 * Author -
 *   Christopher Sean Morrison
 */

#include "common.h"

#include "pkg.h"
#include "brlcad_version.h"


/**
 * returns the compile-time version of libpkg
 */
const char *
pkg_version(void)
{
    return brlcad_ident("The BRL-CAD Network Package (PKG) Library");
}

