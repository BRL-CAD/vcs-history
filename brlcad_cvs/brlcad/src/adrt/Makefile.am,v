1.27
log
@welcome to the "always build" club, libcommon and isst.
@
text
@# $Id: Makefile.am,v 1.26 2007/12/18 17:02:54 brlcad Exp $

adrtd = \
	bench \
	rise

always_compile = \
	libtie \
	libtienet \
	libtexture \
	librender \
	libcommon \
	libutil \
	isst

if BUILD_ADRT
SUBDIRS = \
	${always_compile} \
	${adrtd}
else
SUBDIRS = ${always_compile}
endif

DIST_SUBDIRS = \
	${adrtd} \
	${always_compile} \
	doc \
	misc \
	scripts

tieincludedir = $(includedir)/tie
tieinclude_HEADERS = adrt.h adrt_struct.h

if HAVE_DOXYGEN
doxygen:
	mkdir doxygen
	doxygen Doxyfile
endif

EXTRA_DIST = \
	AUTHORS \
	COPYING \
	Doxyfile \
	NEWS \
	README

DEPS = src/libbu

include $(top_srcdir)/misc/Makefile.defs
@


