1.15
log
@use bu_avail_cpu()s instead of the FreeBSD-only get_procs() version
@
text
@# $Id: Makefile.am,v 1.14 2007/12/05 22:01:38 erikgreenwald Exp $

lib_LTLIBRARIES = libtieutil.la
libtieutil_la_SOURCES = camera.c display.c image.c rand.c umath.c
AM_CFLAGS = @@SDL_CFLAGS@@ -I${top_srcdir}/src/adrt -I${top_srcdir}/src/adrt/libtie -I${top_srcdir}/src/adrt/libtienet -I${top_srcdir}/src/adrt/libcommon -I${top_srcdir}/src/adrt/librender -I${top_srcdir}/src/adrt/libtexture
libtieutil_la_LIBADD= @@SDL_LIBS@@ $(top_builddir)/src/libbu/libbu.la

noinst_HEADERS = camera.h display.h font.h

tieincludedir = $(includedir)/tie
tieinclude_HEADERS = image.h umath.h rand.h 

include $(top_srcdir)/misc/Makefile.defs
@


