#if ! defined( lint )
static const char RCSid[] = "@@(#)$Header: /cvsroot/brlcad/brlcad/src/burst/HmGetc.c,v 14.8 2007/07/26 01:13:50 brlcad Exp $ (BRL)";
#endif
#include <stdio.h>
#include <signal.h>
#include "./burst.h"
#include "./Hm.h"
/*LINTLIBRARY*/
int
HmGetchar( void )
{
    int	c;

    while( (c = getc( HmTtyFp )) == EOF )
	;

    return c;
}

int
HmUngetchar( int c )
{
    return	ungetc( c, HmTtyFp );
}

/*
 * Local Variables:
 * mode: C
 * tab-width: 8
 * c-basic-offset: 4
 * indent-tabs-mode: t
 * End:
 * ex: shiftwidth=4 tabstop=8
 */
@


