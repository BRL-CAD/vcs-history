@/* Machine Generated Real Split Radix Decimation in Freq Inverse FFT */
#define	INVSQ2	0.70710678118654752440
void
irfft16(x)
register double x[];
{
	register double	t1, t2, t3, t4, t5;	register int	i;
/* L shaped butterflies */
t1 = x[0] - x[8];
x[0] += x[8];
x[4] *= 2.0;
x[8] = t1 - 2.0 * x[12];
x[12] = t1 + 2.0 * x[12];
t1 = (x[6] - x[2]) * INVSQ2;
t2 = (x[14] + x[10]) * INVSQ2;
x[2] += x[6];
x[6] = x[14] - x[10];
x[10] = -2.0 * ( t2 + t1 );
x[14] = 2.0 * ( -t2 + t1 );
t1 = x[1] - x[7];
x[1] += x[7];
t2 = x[3] - x[5];
x[3] += x[5];
t3 = x[15] + x[9];
x[7] = x[15] - x[9];
t4 = x[13] + x[11];
x[5] = x[13] - x[11];
t5 = t1 - t4;
t1 += t4;
t4 = t2 - t3;
t2 += t3;
x[9] = t5 * 0.923879532511286738483136 + t4 * 0.382683432365089781779233;
x[11] = - t4 * 0.923879532511286738483136 + t5 * 0.382683432365089781779233;
x[13] = t1 * 0.382683432365089837290384 - t2 * 0.923879532511286738483136;
x[15] = t2 * 0.382683432365089837290384 + t1 * 0.923879532511286738483136;
t1 = x[0] - x[4];
x[0] += x[4];
x[2] *= 2.0;
x[4] = t1 - 2.0 * x[6];
x[6] = t1 + 2.0 * x[6];
t1 = (x[3] - x[1]) * INVSQ2;
t2 = (x[7] + x[5]) * INVSQ2;
x[1] += x[3];
x[3] = x[7] - x[5];
x[5] = -2.0 * ( t2 + t1 );
x[7] = 2.0 * ( -t2 + t1 );
t1 = x[0] - x[2];
x[0] += x[2];
x[1] *= 2.0;
x[2] = t1 - 2.0 * x[3];
x[3] = t1 + 2.0 * x[3];
t1 = x[8] - x[10];
x[8] += x[10];
x[9] *= 2.0;
x[10] = t1 - 2.0 * x[11];
x[11] = t1 + 2.0 * x[11];
t1 = x[12] - x[14];
x[12] += x[14];
x[13] *= 2.0;
x[14] = t1 - 2.0 * x[15];
x[15] = t1 + 2.0 * x[15];
/* Length two butterflies */
t1 = x[0];
x[0] = t1 + x[1];
x[1] = t1 - x[1];
t1 = x[4];
x[4] = t1 + x[5];
x[5] = t1 - x[5];
t1 = x[8];
x[8] = t1 + x[9];
x[9] = t1 - x[9];
t1 = x[12];
x[12] = t1 + x[13];
x[13] = t1 - x[13];
t1 = x[6];
x[6] = t1 + x[7];
x[7] = t1 - x[7];
/* bit reverse */
t1 = x[8];
x[8] = x[1];
x[1] = t1;
t1 = x[4];
x[4] = x[2];
x[2] = t1;
t1 = x[12];
x[12] = x[3];
x[3] = t1;
t1 = x[10];
x[10] = x[5];
x[5] = t1;
t1 = x[14];
x[14] = x[7];
x[7] = t1;
t1 = x[13];
x[13] = x[11];
x[11] = t1;
/* scale result */
for( i = 0; i < 16; i++ )
	x[i] /= 16.000000;
}
@


