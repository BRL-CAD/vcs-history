#ifndef lint
static const char libbu_memset_RCSid[] = "@@(#)$Header: /cvsroot/brlcad/brlcad/src/libbu/memset.c,v 14.9 2006/01/18 06:46:16 brlcad Exp $ (ARL)";
#endif

#if defined(BSD) && (BSD < 43)

char *
memset( s, c, n )
char	*s;
register char	c;
register int	n;
{
	register char *p=s;

	if (p)
		while (n-- > 0)
			*p++ = c;

	return s;
}

#endif

