1.1.2.5
log
@generate tclIndex via makefile instead of using ami.tcl
@
text
@d1 1
a1 1
# $Id: Makefile.am,v 1.1.2.4 2004/01/06 14:22:19 erikg Exp $
d10 2
a11 3
.PHONY : tclIndex
tclIndex: 
	grep '^[ \t]*proc' *tcl *itk | sed 's,\([^:]*\):[ \t]*proc[ \t]*\([a-zA-Z0-9_?/:-][a-zA-Z0-9_?/:-]*\).*,set auto_index(\2) [list source [file join \$$dir \1]],' > tclIndex
@


