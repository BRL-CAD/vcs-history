@# $Id: Makefile.am,v 1.4 2004/04/06 19:32:20 morrison Exp $
SUBDIRS=geometree lib mged nirt pl-dm rtwizard util

tcldir = $(prefix)/tclscripts

# should it be _DATA ? +x or not?)
dist_tcl_SCRIPTS= \
	ami.tcl \
	ampi.tcl \
	cad_clrpick.tcl \
	cad_dialog.tcl \
	chkexterns.tcl \
	fs_dialog.tk \
	helpcomm.tcl \
	helplib.tcl \
	hoc.tcl \
	html_library.tcl \
	libdm.tcl \
	menu_override.tcl \
	mouse.tcl \
	vmath.tcl

install-data-hook:
	${top_builddir}/bwish/btclsh $(top_srcdir)/tclscripts/ami.tcl $(tcldir)

@


