text
@/*
**	Standard definitions for C
**
**	$Header: /cvs/brlcad/cake/std.h,v 11.1 1995/01/05 06:00:01 mike Rel4_4 $
*/

#define	or		else if
#define	loop		for(;;)
#define	until(expr)	while(!(expr))
#ifndef	reg
#define	reg		register
#endif
#ifndef	bool
#define	bool		char
#endif

#define	max(a, b)	((a) > (b) ? (a) : (b))
#define	min(a, b)	((a) < (b) ? (a) : (b))

#define streq(s1, s2)		(strcmp(s1, s2) == 0)
#define strdiff(s1, s2)		(strcmp(s1, s2) != 0)
#define strneq(s1, s2, n)	(strncmp(s1, s2, n) == 0)
#define strndiff(s1, s2, n)	(strncmp(s1, s2, n) != 0)

#define	ungetchar(c)		ungetc(c, stdin)
#define make(type)		((type *) newmem(sizeof(type)))
#define make_many(type, count)	((type *) newmem(count * sizeof(type)))

#ifndef	TRUE
#define	TRUE		1
#endif
#ifndef	FALSE
#define	FALSE		0
#endif
#ifndef	NULL
#define	NULL		0
#endif

#define	CNULL		(char *) 0
#define	LNULL		(List *) 0
@


